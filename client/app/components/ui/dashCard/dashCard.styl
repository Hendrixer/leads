.dash-card
  background-color white
  height 200px
  .title
    background-color #efefef
    width 100%
    text-align center
    margin-bottom 2rem
    *
      margin 0
      font-weight 300
      font-size 2.5rem
      color rgba(0,0,0,.5)
      text-transform capitalize
  .count *
    margin 0
    font-weight bold
    color #00E5FF
    font-size 2.8rem
